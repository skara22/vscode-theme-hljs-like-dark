{
  "name": "HLJS-like Dark (tuned)",
  "type": "dark",
  "semanticHighlighting": true,

  "semanticTokenColors": {
    "property": "#df3079",
    "method": "#df3079",
    "numberLiteral": "#df3079"
  },

  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment.line.double-slash.cpp",
        "punctuation.definition.comment.cpp"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "Strings, includes, encodings",
      "scope": [
        "meta.encoding.cpp",
        "string.quoted.double.cpp",
        "string.quoted.double.raw.cpp",
        "string.quoted.other.lt-gt.include.cpp",
        "string.quoted.single.cpp"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "Constants / numbers / member function",
      "scope": [
        "constant.language.false.cpp",
        "constant.language.true.cpp",
        "constant.numeric.binary.cpp",
        "constant.numeric.decimal.cpp",
        "constant.numeric.hexadecimal.cpp",
        "constant.numeric.octal.cpp",
        "entity.name.function.member.cpp",
        "keyword.other.unit.binary.cpp",
        "keyword.other.unit.hexadecimal.cpp",
        "keyword.other.unit.octal.cpp",
        "entity.name.function.definition.cpp"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "Types (primitive, struct, template, sizeof, logical and)",
      "scope": [
        "entity.name.type.parameter.cpp",
        "entity.name.type.struct.cpp",
        "keyword.operator.and.cpp",
        "meta.arguments.operator.sizeof",
        "storage.type.primitive.cpp"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "class / struct / union / public",
      "scope": [
        "storage.type.class.cpp",
        "storage.type.modifier.access.control.public.cpp",
        "storage.type.struct.cpp",
        "storage.type.union.cpp",
        "storage.modifier.consteval.cpp",
        "keyword.other.constinit.cpp",
        "storage.modifier.constexpr.cpp",
        "keyword.control.co_await.cpp",
        "keyword.control.co_yield.cpp",
        "keyword.control.co_return.cpp",
        "storage.type.namespace.alias.cpp",
        "storage.type.namespace.definition.cpp",
        "storage.type.namespace.directive.cpp",
        "keyword.other.typedef.cpp"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "Operators, attributes, dot/scope access, misc (white)",
      "scope": [
        "entity.name.function.call.cpp",
        "entity.name.function.preprocessor.cpp",
        "entity.name.scope-resolution.cpp",
        "entity.name.type.cpp",

        "entity.other.attribute.deprecated.cpp",
        "entity.other.attribute.maybe_unused.cpp",
        "entity.other.attribute.noreturn.cpp",
        "entity.other.attribute.nodiscard.cpp",

        "keyword.operator.and_eq.cpp",
        "keyword.operator.arithmetic.cpp",
        "keyword.operator.assignment.cpp",
        "keyword.operator.bitor.cpp",
        "keyword.operator.bitwise.shift.cpp",
        "keyword.operator.cast.dynamic_cast.cpp",
        "keyword.operator.comparison.cpp",
        "keyword.operator.compl.cpp",
        "keyword.operator.decrement.cpp",
        "keyword.operator.functionlike.cpp",
        "keyword.operator.increment.cpp",
        "keyword.operator.noexcept.cpp",
        "keyword.operator.not.cpp",
        "keyword.operator.wordlike.cpp",

        "meta.arguments.operator.alignof",
        "meta.function.definition.cpp",

        "punctuation.separator.dot-access.cpp",
        "punctuation.separator.scope-resolution.cpp",

        "storage.modifier.reference.cpp",
        "storage.modifier.virtual.cpp",
        "storage.modifier.specifier.functional.pre-parameters.inline.cpp",

        "storage.type.modifier.access.control.protected.cpp",

        "support.other.attribute.cpp",

        "variable.lower-case.cpp",
        "variable.other.object.access.d.cpp",
        "variable.snake-case.cpp"
      ],
      "settings": {
        "foreground": "#FDFDFDFF"
      }
    },
    {
      "name": "Preprocessor / directives / if / return / static_assert",
      "scope": [
        "keyword.control.directive.conditional.defined.cpp",
        "keyword.control.directive.conditional.else.cpp",
        "keyword.control.directive.conditional.if.cpp",
        "keyword.control.directive.conditional.ifndef.cpp",
        "keyword.control.directive.define.cpp",
        "keyword.control.directive.else.cpp",
        "keyword.control.directive.elif.cpp",
        "keyword.control.directive.endif.cpp",
        "keyword.control.directive.include.cpp",
        "keyword.control.directive.pragma.cpp",
        "keyword.control.directive.undef.cpp",

        "keyword.control.if.cpp",
        "keyword.control.return.cpp",

        "keyword.other.default.constructor.cpp",
        "keyword.other.static-assert.cpp",

        "meta.preprocessor.macro.cpp",
        "punctuation.definition.directive.cpp"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "Keywords / modifiers / control flow (blue)",
      "scope": [
        "keyword.control.break.cpp",
        "keyword.control.case.cpp",
        "keyword.control.continue.cpp",
        "keyword.control.default.cpp",
        "keyword.control.do.cpp",
        "keyword.control.for.cpp",
        "keyword.control.goto.cpp",
        "keyword.control.switch.cpp",
        "keyword.control.throw.cpp",
        "keyword.control.while.cpp",

        "keyword.operator.sizeof.cpp",

        "keyword.other.concept.cpp",
        "keyword.other.default.destructor.cpp",
        "keyword.other.export.cpp",
        "keyword.other.module.cpp",
        "keyword.other.operator.overload.cpp",
        "keyword.other.using.directive.cpp",

        "parameters.virtual.cpp",

        "storage.modifier.lambda.constexpr.cpp",
        "storage.modifier.specifier.const.cpp",
        "storage.modifier.specifier.functional.post-parameters.const.cpp",
        "storage.modifier.specifier.functional.post-parameters.noexcept.cpp",
        "storage.modifier.specifier.functional.post-parameters.override.cpp",
        "storage.modifier.specifier.functional.pre-parameters.constexpr.cpp",
        "storage.modifier.specifier.functional.pre-parameters.explicit.cpp",
        "storage.modifier.specifier.functional.pre-parameters.friend.cpp",
        "storage.modifier.specifier.functional.pre-parameters.mutable.cpp",
        "storage.modifier.specifier.functional.pre-parameters.virtual.cpp",
        "storage.modifier.specifier.static.cpp",
        "storage.modifier.specifier.thread_local.cpp",
        "storage.modifier.specifier.volatile.cpp",

        "storage.type.template.argument.typename.cpp",
        "storage.type.template.cpp",

        "variable.other.unknown.import.cpp"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "Labels",
      "scope": [
        "entity.name.label.call.cpp",
        "entity.name.label.cpp",
        "variable.other.unknown.label1.cpp"
      ],
      "settings": {
        "foreground": "#f22c3d"
      }
    },
    {
      "name": "HTML comments",
      "scope": [
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "HTML attributes",
      "scope": [
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "HTML text",
      "scope": [
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#FFFFFFFF",
      }
    },
    {
      "name": "HTML atribute",
      "scope": [
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "HTML strings",
      "scope": [
        "punctuation.definition.string.begin.html",
        "string.quoted.double.html"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "HTML script tag",
      "scope": [
        "meta.tag.metadata.script.start.html",
        "meta.tag.metadata.script.end.html",
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "HTML style tag",
      "scope": [
        "meta.tag.metadata.style.start.html",
        "meta.tag.metadata.style.end.html"
      ],
      "settings": {
        "foreground": "#C586C0FF"
      }
    },
    {
      "name": "CSS comments",
      "scope": [
        "comment.block.css"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "CSS base",
      "scope": [
        "entity.name.tag.css",
        "keyword.operator.combinator.css",
        "constant.numeric.css",
        "punctuation.separator.list.comma.css"
      ],
      "settings": {
        "foreground": "#FDFDFDFF"
      }
    },
    {
      "name": "CSS selectors / variables",
      "scope": [
        "meta.selector.css",
        "variable.css"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "CSS properties / strings / media",
      "scope": [
        "meta.property-name.css",
        "string.quoted.double.css",
        "support.type.property-name.media.css"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "CSS id selectors",
      "scope": [
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f22c3d"
      }
    },
    {
      "name": "CSS at-rules",
      "scope": [
        "punctuation.definition.keyword.css",
        "keyword.control.at-rule.media.css"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "CSS color functions",
      "scope": [
        "meta.function.color.css"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "JavaScript comments",
      "scope": [
        "comment.line.double-slash.js",
        "comment.block.js"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "JavaScript types / storage",
      "scope": [
        "storage.type.js",
        "storage.type.function.js",
        "storage.type.property.js",
        "storage.modifier.async.js"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "JavaScript numbers / properties",
      "scope": [
        "constant.numeric.decimal.js",
        "variable.other.property.js"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "JavaScript strings",
      "scope": [
        "string.quoted.single.js",
        "string.quoted.double.js"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "JavaScript keywords / flow",
      "scope": [
        "constant.language.boolean.true.js",
        "constant.language.boolean.false.js",
        "constant.language.undefined.js",
        "constant.language.null.js",
        "storage.type.class.js",
        "storage.modifier.js",
        "variable.language.this.js",
        "new.expr.js",
        "keyword.control.loop.js",
        "keyword.control.switch.js",
        "keyword.control.trycatch.js"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "JavaScript base / object keys",
      "scope": [
        "source.js",
        "meta.object-literal.key.js",
        "string.template.js",
        "meta.block.js",
        "variable.object.property.js",
        "keyword.control.flow.js"
      ],
      "settings": {
        "foreground": "#FDFDFDFF"
      }
    },
    {
      "name": "PHP base",
      "scope": [
        "source.php",
        "text.html.php"
      ],
      "settings": {
        "foreground": "#FDFDFDFF"
      }
    },
    {
      "name": "PHP comments",
      "scope": [
        "comment.line.double-slash.php"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "PHP keywords / flow / types",
      "scope": [
        "keyword.control.declare.php",
        "keyword.other.namespace.php",
        "keyword.other.use.php",
        "storage.type.function.php",
        "storage.type.const.php",
        "storage.type.class.php",
        "storage.modifier.php",
        "storage.type.enum.php",
        "storage.type.interface.php",
        "constant.language.php",
        "keyword.control.return.php",
        "keyword.control.if.php",
        "keyword.control.else.php",
        "keyword.control.for.php",
        "keyword.control.foreach.php",
        "keyword.control.do.php",
        "keyword.control.while.php",
        "keyword.control.exception.catch.php",
        "keyword.control.exception.php",
        "support.function.construct.output.php"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    },
    {
      "name": "PHP strings / namespaces",
      "scope": [
        "entity.name.type.namespace.php",
        "punctuation.definition.string.begin.php",
        "string.quoted.single.php",
        "string.quoted.double.php",
        "string.unquoted.heredoc.php",
        "punctuation.definition.string.php",
        "string.unquoted.nowdoc.php"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "PHP classes / functions / helpers",
      "scope": [
        "support.class.builtin.php",
        "support.class.php",
        "entity.name.type.class.php",
        "entity.name.function.php",
        "support.function.var.php",
        "support.function.info.php"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "PHP enums / interfaces / constants",
      "scope": [
        "constant.other.class.php",
        "entity.name.type.enum.php",
        "keyword.other.type.php",
        "entity.name.type.interface.php",
        "constant.numeric.decimal.php"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "Json/Jsonc base",
      "scope": [
        "source.json",
        "source.json.comments",
        "source.jsonc"
      ],
      "settings": {
        "foreground": "#FDFDFDFF"
      }
    },
    {
      "name": "Json/Jsonc comment",
      "scope": [
        "punctuation.definition.comment.json",
        "punctuation.definition.comment.json.comments",
        "punctuation.definition.comment.jsonc"
      ],
      "settings": {
        "foreground": "#9b9b9b"
      }
    },
    {
      "name": "Json/Jsonc string",
      "scope": [
        "string.json",
        "string.json.comments",
        "string.comment.jsonc"
      ],
      "settings": {
        "foreground": "#e9950c"
      }
    },
    {
      "name": "Json/Jsonc string double",
      "scope": [
        "string.quoted.double.json",
        "string.quoted.double.json.comments",
        "string.quoted.double.comment.jsonc"
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "Json/Jsonc numeric",
      "scope": [
        "constant.numeric.json",
        "constant.numeric.json.comments",
        "constant.numeric.comment.jsonc"
      ],
      "settings": {
        "foreground": "#df3079"
      }
    },
    {
      "name": "Json/Jsonc boolean",
      "scope": [
        "constant.language.json",
        "constant.language.json.comments",
        "constant.language.comment.jsonc"
      ],
      "settings": {
        "foreground": "#2e95d3"
      }
    }
    
  ],

